{
    "name": "goldfinch/taz",
    "description": "Command Line Interface (CLI) for Silverstripe, to help and speed up development process",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "silverstripe-cli",
        "silverstripe-taz",
        "command-line-interface"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Art Karasev",
            "email": "art.karasev@proton.me"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^6.0",
        "silverstripe/admin": "^3.0",
        "symfony/console": "^6.3",
        "hackzilla/password-generator": "^1.6",
        "consolidation/comments": "^1.0",
        "minwork/array": "^1.13",
        "goldfinch/helpers": "^2.0",
        "goldfinch/cli-supplier": "^2.0",
        "goldfinch/illuminate": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Taz\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
